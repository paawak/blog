<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="CONSOLE" target="SYSTEM_OUT">
			<PatternLayout pattern="%p %c{10}.%M(%L) | %m%n" />
		</Console>
	</Appenders>

	<Loggers>
		<root level="debug">
			<AppenderRef ref="CONSOLE" />
		</root>
	</Loggers>

</Configuration>
